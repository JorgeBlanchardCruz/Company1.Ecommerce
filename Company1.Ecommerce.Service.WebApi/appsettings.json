{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "NorthwindConnection": "NorthwindConnection",
    "RedisConnection": "localhost:6379,user=default,password=123456,ssl=False,abortConnect=False"
  },
  "Config": {
    "OriginCors": "http://localhost:5285/",
    "SecretKey": "SecretKey",
    "Issuer": "Issuer",
    "Audience": "Issuer"
  },
  "HealthChecksUI": {
    "HealthChecks": [
      {
        "Name": "Healt Check API",
        "Uri": "/health"
      }
    ],
    "EvaluationTimeInSeconds": 5
  },
  "WatchDog": {
    "WatchPageUsername": "admin",
    "WatchPagePassword": "123456"
  },
  "RateLimiting": {
    "PermitLimit": 4,
    "Window": 30,
    "QueueLimit": 0
  }
}
